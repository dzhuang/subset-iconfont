// bordered icons (extracted from _bordered-pulled.scss)
// -------------------------

.#{${{ prefix }}-css-prefix}-border {
  border-color: var(--#{${{ prefix }}-css-prefix}-border-color, #{${{ prefix }}-border-color});
  border-radius: var(--#{${{ prefix }}-css-prefix}-border-radius, #{${{ prefix }}-border-radius});
  border-style: var(--#{${{ prefix }}-css-prefix}-border-style, #{${{ prefix }}-border-style});
  border-width: var(--#{${{ prefix }}-css-prefix}-border-width, #{${{ prefix }}-border-width});
  padding: var(--#{${{ prefix }}-css-prefix}-border-padding, #{${{ prefix }}-border-padding});
}
